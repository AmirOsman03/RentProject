{"ast":null,"code":"export default {\n  data() {\n    return {\n      showModal: false,\n      showEditModal: false,\n      selectedCar: null,\n      bookingData: {\n        name: '',\n        date: ''\n      },\n      currentUser: JSON.parse(localStorage.getItem(\"user\")) || {\n        role: \"user\"\n      }\n    };\n  },\n  methods: {\n    openModal(car) {\n      console.log(\"Opening modal for\", car);\n      this.selectedCar = car;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n    },\n    bookCar() {\n      console.log(\"Booking car:\", this.selectedCar, \"for\", this.bookingData);\n      alert(`Booking confirmed for ${this.selectedCar.name} on ${this.bookingData.date}`);\n      this.closeModal();\n    },\n    deleteCar() {\n      if (this.currentUser.role === \"admin\") {\n        console.log(\"Car deleted:\", this.selectedCar);\n        alert(\"Car deleted successfully!\");\n      } else {\n        alert(\"You do not have permission to delete this car.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","showModal","showEditModal","selectedCar","bookingData","name","date","currentUser","JSON","parse","localStorage","getItem","role","methods","openModal","car","console","log","closeModal","bookCar","alert","deleteCar"],"sources":["src/views/LoginPage.vue"],"sourcesContent":["<template>\n  <div>\n    <button @click=\"openModal(car)\">Book Car</button>\n\n    <div v-if=\"showModal\" class=\"modal-overlay\">\n      <div class=\"modal-content\">\n        <h2>Book {{ selectedCar?.name }}</h2>\n        <form @submit.prevent=\"bookCar\">\n          <label for=\"name\">Your Name:</label>\n          <input type=\"text\" v-model=\"bookingData.name\" required />\n\n          <label for=\"date\">Pick-up Date:</label>\n          <input type=\"date\" v-model=\"bookingData.date\" required />\n\n          <button type=\"submit\">Confirm Booking</button>\n          <button type=\"button\" @click=\"closeModal\">Cancel</button>\n        </form>\n      </div>\n    </div>\n\n    <div v-if=\"currentUser.role === 'admin'\" class=\"admin-panel\">\n      <h2>Admin Panel</h2>\n      <button @click=\"showEditModal = true\">Edit Car</button>\n      <button @click=\"deleteCar\">Delete Car</button>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      showModal: false,\n      showEditModal: false,\n      selectedCar: null,\n      bookingData: {\n        name: '',\n        date: ''\n      },\n      currentUser: JSON.parse(localStorage.getItem(\"user\")) || { role: \"user\" },\n    };\n  },\n  methods: {\n    openModal(car) {\n      console.log(\"Opening modal for\", car);\n      this.selectedCar = car;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n    },\n    bookCar() {\n      console.log(\"Booking car:\", this.selectedCar, \"for\", this.bookingData);\n      alert(`Booking confirmed for ${this.selectedCar.name} on ${this.bookingData.date}`);\n      this.closeModal();\n    },\n    deleteCar() {\n      if (this.currentUser.role === \"admin\") {\n        console.log(\"Car deleted:\", this.selectedCar);\n        alert(\"Car deleted successfully!\");\n      } else {\n        alert(\"You do not have permission to delete this car.\");\n      }\n    }\n  }\n};\n</script>\n\n<style>\n.modal-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.modal-content {\n  background: white;\n  padding: 20px;\n  border-radius: 8px;\n  text-align: center;\n}\n\n.admin-panel {\n  margin-top: 20px;\n  padding: 10px;\n  background: #333;\n  color: white;\n  text-align: center;\n  border-radius: 8px;\n}\n</style>\n"],"mappings":"AA6BA;EACAA,KAAA;IACA;MACAC,SAAA;MACAC,aAAA;MACAC,WAAA;MACAC,WAAA;QACAC,IAAA;QACAC,IAAA;MACA;MACAC,WAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;QAAAC,IAAA;MAAA;