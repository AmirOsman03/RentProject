 </div>\n</template>\n\n<script>\nexport default {\n  name: \"MyBookings\",\n  data() {\n    return {\n      bookings: this.getBookingsFromLocalStorage(),\n      newBooking: {\n        car: {\n          brand: \"Toyota\",\n          model: \"Corolla\",\n        },\n        startDate: \"2025-02-01\",\n        endDate: \"2025-02-10\",\n        totalPrice: 200,\n      },\n    };\n  },\n  methods: {\n    addBooking() {\n      // Add new booking to the array\n      this.bookings.push(this.newBooking);\n\n      // Save the updated bookings to localStorage\n      localStorage.setItem(\"myBookings\", JSON.stringify(this.bookings));\n    },\n    getBookingsFromLocalStorage() {\n      // Get bookings from localStorage, if exists\n      const savedBookings = localStorage.getItem(\"myBookings\");\n      return savedBookings ? JSON.parse(savedBookings) : [];\n    },\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  margin-top: 20px;\n}\n.card {\n  border: 1px solid #ccc;\n  margin-bottom: 15px;\n}\n</style>\n"],"mappings":";AA4BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,QAAA,OAAAC,2BAAA;MACAC,UAAA;QACAC,GAAA;UACAC,KAAA;UACAC,KAAA;QACA;QACAC,SAAA;QACAC,OAAA;QACAC,UAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA;MACA,KAAAV,QAAA,CAAAW,IAAA,MAAAT,UAAA;;MAEA;MACAU,YAAA,CAAAC,OAAA,eAAAC,IAAA,CAAAC,SAAA,MAAAf,QAAA;IACA;IACAC,4BAAA;MACA;MACA,MAAAe,aAAA,GAAAJ,YAAA,CAAAK,OAAA;MACA,OAAAD,aAAA,GAAAF,IAAA,CAAAI,KAAA,CAAAF,aAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}