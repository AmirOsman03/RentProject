{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  name: \"BookingsPage\",\n  data() {\n    return {\n      bookings: [{\n        id: 1,\n        car: 'Toyota Corolla',\n        date: '2025-02-01'\n      }, {\n        id: 2,\n        car: 'BMW X5',\n        date: '2025-02-05'\n      }]\n    };\n  },\n  methods: {\n    cancelBooking(id) {\n      this.bookings = this.bookings.filter(booking => booking.id !== id);\n      alert('Booking cancelled successfully!');\n    }\n  }\n};","map":{"version":3,"names":["name","data","bookings","id","car","date","methods","cancelBooking","filter","booking","alert"],"sources":["src/views/BookingsPage.vue"],"sourcesContent":["<template>\n  <div class=\"p-4\">\n    <h1 class=\"text-2xl font-bold mb-4\">Bookings</h1>\n\n    <div v-if=\"bookings.length === 0\" class=\"text-gray-600\">\n      No bookings available.\n    </div>\n\n    <div v-else class=\"grid grid-cols-1 gap-4\">\n      <div\n        v-for=\"booking in bookings\"\n        :key=\"booking.id\"\n        class=\"border rounded-lg p-4 bg-white shadow-md flex justify-between items-center\"\n      >\n        <div>\n          <h2 class=\"font-semibold text-lg\">{{ booking.car }}</h2>\n          <p class=\"text-gray-600\">{{ booking.date }}</p>\n        </div>\n\n        <button\n          @click=\"cancelBooking(booking.id)\"\n          class=\"bg-red-500 hover:bg-red-600 text-white px-4 py-2 rounded-lg\"\n        >\n          Cancel\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"BookingsPage\",\n  data() {\n    return {\n      bookings: [\n        { id: 1, car: 'Toyota Corolla', date: '2025-02-01' },\n        { id: 2, car: 'BMW X5', date: '2025-02-05' },\n      ],\n    };\n  },\n  methods: {\n    cancelBooking(id) {\n      this.bookings = this.bookings.filter(booking => booking.id !== id);\n      alert('Booking cancelled successfully!');\n    },\n  },\n};\n</script>\n\n<style scoped>\n.grid {\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n}\n</style>\n"],"mappings":";;AA+BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,QAAA,GACA;QAAAC,EAAA;QAAAC,GAAA;QAAAC,IAAA;MAAA,GACA;QAAAF,EAAA;QAAAC,GAAA;QAAAC,IAAA;MAAA;IAEA;EACA;EACAC,OAAA;IACAC,cAAAJ,EAAA;MACA,KAAAD,QAAA,QAAAA,QAAA,CAAAM,MAAA,CAAAC,OAAA,IAAAA,OAAA,CAAAN,EAAA,KAAAA,EAAA;MACAO,KAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}