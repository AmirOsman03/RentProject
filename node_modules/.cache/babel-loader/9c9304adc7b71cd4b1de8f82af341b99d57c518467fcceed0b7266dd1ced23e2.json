         <button class=\"btn btn-primary w-100\" @click=\"openModal(car)\">Book Now</button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Модално прозорче за резервација -->\n    <div v-if=\"isModalOpen\" class=\"modal-overlay\">\n      <div class=\"modal-content\">\n        <h2>Reservation for {{ selectedCar.brand }} {{ selectedCar.model }}</h2>\n        <form @submit.prevent=\"submitForm\">\n          <div class=\"form-group\">\n            <label for=\"start-date\">Start Date</label>\n            <input type=\"date\" id=\"start-date\" v-model=\"startDate\" required />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"end-date\">End Date</label>\n            <input type=\"date\" id=\"end-date\" v-model=\"endDate\" required />\n          </div>\n          <button type=\"submit\" class=\"btn btn-success\">Submit</button>\n          <button @click=\"closeModal\" type=\"button\" class=\"btn btn-secondary\">Close</button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'CarList',\n  data() {\n    return {\n      cars: [\n        { id: 1, brand: 'Mercedes-Benz', model: 'C63', productionYear: 2003, type: 'COUPE', price: 300, image: 'c63.jpg' },\n        { id: 2, brand: 'BMW', model: 'M3', productionYear: 2019, type: 'SEDAN', price: 400, image: 'm3.jpg' },\n        { id: 3, brand: 'Audi', model: 'RS5', productionYear: 2018, type: 'COUPE', price: 300, image: 'rs5.jpg' },\n        { id: 4, brand: 'Volkswagen', model: 'Golf R', productionYear: 2022, type: 'HATCHBACK', price: 200, image: 'golf.jpg' },\n        { id: 5, brand: 'Toyota', model: 'Supra', productionYear: 2021, type: 'COUPE', price: 300, image: 'supra.jpg' },\n        { id: 6, brand: 'Porsche', model: '911 Turbo S', productionYear: 2023, type: 'COUPE', price: 250, image: '911.jpg' },\n        { id: 7, brand: 'Ford', model: 'Mustang GT', productionYear: 2020, type: 'COUPE', price: 350, image: 'mustang.jpg' },\n        { id: 8, brand: 'Lamborghini', model: 'Huracan', productionYear: 2022, type: 'COUPE', price: 500, image: 'huracan.jpg' },\n        { id: 9, brand: 'Ferrari', model: 'F8 Tributo', productionYear: 2021, type: 'COUPE', price: 500, image: 'f8.jpg' },\n        { id: 10, brand: 'Nissan', model: 'GT-R', productionYear: 2020, type: 'COUPE', price: 250, image: 'nissan.jpg' },\n      ],\n      isModalOpen: false, // Статус на модалното прозорче\n      selectedCar: null, // Избран автомобил за резервација\n      startDate: '',\n      endDate: ''\n    };\n  },\n  methods: {\n    openModal(car) {\n      this.selectedCar = car; // Постави го избраниот автомобил\n      this.isModalOpen = true; // Отвори го модалното прозорче\n    },\n    closeModal() {\n      this.isModalOpen = false; // Затвори го модалното прозорче\n      this.startDate = ''; // Ресетирај ги полето за датуми\n      this.endDate = '';\n    },\n    submitForm() {\n      // Логика за обработка на податоците, како што е испраќање на резервацијата на сервер\n      console.log(\"Reservation for car:\", this.selectedCar);\n      console.log(\"Start date:\", this.startDate);\n      console.log(\"End date:\", this.endDate);\n\n      // Затвори ја формата по успешен поднесок\n      this.closeModal();\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Style your cards and images as needed */\n.card-img-top {\n  width: 100%;\n  height: 200px;\n  object-fit: cover;\n}\n\n/* Modals and form styles */\n.modal-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  z-index: 1000;\n}\n\n.modal-content {\n  background-color: white;\n  padding: 20px;\n  border-radius: 8px;\n  width: 400px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n}\n\nbutton {\n  margin-top: 10px;\n}\n\n.form-group {\n  margin-bottom: 15px;\n}\n</style>\n"],"mappings":"AA8CA;EACAA,IAAA;EACAC,KAAA;IACA;MAC