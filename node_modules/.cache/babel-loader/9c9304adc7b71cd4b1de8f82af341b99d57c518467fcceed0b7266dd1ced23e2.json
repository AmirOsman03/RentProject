{"ast":null,"code":"export default {\n  name: 'CarList',\n  data() {\n    return {\n      cars: [{\n        id: 1,\n        brand: 'Mercedes-Benz',\n        model: 'C63',\n        productionYear: 2003,\n        type: 'COUPE',\n        price: 300,\n        image: 'c63.jpg'\n      }, {\n        id: 2,\n        brand: 'BMW',\n        model: 'M3',\n        productionYear: 2019,\n        type: 'SEDAN',\n        price: 400,\n        image: 'm3.jpg'\n      }, {\n        id: 3,\n        brand: 'Audi',\n        model: 'RS5',\n        productionYear: 2018,\n        type: 'COUPE',\n        price: 300,\n        image: 'rs5.jpg'\n      }, {\n        id: 4,\n        brand: 'Volkswagen',\n        model: 'Golf R',\n        productionYear: 2022,\n        type: 'HATCHBACK',\n        price: 200,\n        image: 'golf.jpg'\n      }, {\n        id: 5,\n        brand: 'Toyota',\n        model: 'Supra',\n        productionYear: 2021,\n        type: 'COUPE',\n        price: 300,\n        image: 'supra.jpg'\n      }, {\n        id: 6,\n        brand: 'Porsche',\n        model: '911 Turbo S',\n        productionYear: 2023,\n        type: 'COUPE',\n        price: 250,\n        image: '911.jpg'\n      }, {\n        id: 7,\n        brand: 'Ford',\n        model: 'Mustang GT',\n        productionYear: 2020,\n        type: 'COUPE',\n        price: 350,\n        image: 'mustang.jpg'\n      }, {\n        id: 8,\n        brand: 'Lamborghini',\n        model: 'Huracan',\n        productionYear: 2022,\n        type: 'COUPE',\n        price: 500,\n        image: 'huracan.jpg'\n      }, {\n        id: 9,\n        brand: 'Ferrari',\n        model: 'F8 Tributo',\n        productionYear: 2021,\n        type: 'COUPE',\n        price: 500,\n        image: 'f8.jpg'\n      }, {\n        id: 10,\n        brand: 'Nissan',\n        model: 'GT-R',\n        productionYear: 2020,\n        type: 'COUPE',\n        price: 250,\n        image: 'nissan.jpg'\n      }],\n      isModalOpen: false,\n      // Статус на модалн