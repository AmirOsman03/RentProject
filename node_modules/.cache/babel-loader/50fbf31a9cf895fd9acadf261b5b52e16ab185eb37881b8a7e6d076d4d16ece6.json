{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport { cars } from \"@/cars\";\nexport default {\n  name: \"CarList\",\n  data() {\n    return {\n      showAddCarModal: false,\n      newCar: {\n        brand: \"\",\n        model: \"\",\n        price: 0,\n        type: \"\",\n        fuelType: \"\"\n      },\n      cars: cars,\n      selectedType: \"\",\n      selectedFuelType: \"\",\n      showModal: false,\n      showEditModal: false,\n      selectedCar: null,\n      startDate: \"\",\n      endDate: \"\",\n      editedCar: {},\n      currentUser: {\n        role: \"admin\" // Читање од localStorage\n      },\n      myBookings: []\n    };\n  },\n  computed: {\n    filteredCars() {\n      return this.cars.filter(car => {\n        const matchesType = !this.selectedType || car.type === this.selectedType;\n        const matchesFuelType = !this.selectedFuelType || car.fuelType === this.selectedFuelType;\n        return matchesType && matchesFuelType;\n      });\n    },\n    totalPrice() {\n      if (this.startDate && this.endDate && this.selectedCar) {\n        const start = new Date(this.startDate);\n        const end = new Date(this.endDate);\n        const timeDiff = end - start;\n        const dayCount = timeDiff / (1000 * 3600 * 24);\n        if (dayCount < 1) return 0;\n        return this.selectedCar.price * dayCount;\n      }\n      return 0;\n    }\n  },\n  methods: {\n    openAddCarModal() {\n      this.showAddCarModal = true;\n      this.newCar = {\n        brand: \"\",\n        model: \"\",\n        price: \"\",\n        type: \"COUPE\",\n        fuelType: \"Petrol\"\n      };\n    },\n    closeAddCarModal() {\n      this.showAddCarModal = false;\n    },\n    addNewCar() {\n      if (!this.newCar.brand || !this.newCar.model || !this.newCar.price || !this.newCar.type || !this.newCar.fuelType) {\n        alert(\"Please fill all fields.\");\n        return;\n      }\n      const newCar = {\n        id: this.cars.length + 1,\n        ...this.newCar\n      };\n\n      // Додавање на новото возило\n      this.cars.push(newCar);\n\n      // Затворање на модалот\n      this.closeAddCarModal();\n      alert(`You have successfully added the ${newCar.brand} ${newCar.model}`);\n    },\n    openModal(car) {\n      this.selectedCar = car;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n      this.startDate = \"\";\n      this.endDate = \"\";\n      this.selectedCar = null;\n    },\n    confirmBooking() {\n      if (!this.startDate || !this.endDate) {\n        alert(\"Please select both start and end dates for the booking.\");\n        return;\n      }\n      if (this.endDate < this.startDate) {\n        alert(\"End date cannot be earlier than start date.\");\n        return;\n      }\n      const confirmation = window.confirm(`Are you sure you want to book from ${this.startDate} to ${this.endDate}?`);\n      if (confirmation) {\n        // Dodaj ja rezervacijata vo \"myBookings\"\n        const booking = {\n          car: this.selectedCar,\n          startDate: this.startDate,\n          endDate: this.endDate,\n          totalPrice: this.totalPrice\n        };\n        this.myBookings.push(booking); // Dodavanje vo lista\n        alert(`You have successfully booked the ${this.selectedCar.brand} ${this.selectedCar.model}`);\n        this.closeModal();\n      } else {\n        alert(\"Booking canceled.\");\n      }\n    },\n    deleteCar(carId) {\n      this.cars = this.cars.filter(car => car.id !== carId);\n    },\n    saveEditedCar() {\n      const index = this.cars.findIndex(car => car.id === this.editedCar.id);\n      if (index !== -1) {\n        this.cars.splice(index, 1, {\n          ...this.editedCar\n        });\n      }\n      this.showEditModal = false;\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n    },\n    openEditModal(car) {\n      this.editedCar = {\n        ...car\n      };\n      this.showEditModal = true;\n    }\n  }\n};","map":{"version":3,"names":["cars","name","data","showAddCarModal","newCar","brand","model","price","type","fuelType","selectedType","selectedFuelType","showModal","showEditModal","selectedCar","startDate","endDate","editedCar","currentUser","role","myBookings","computed","filteredCars","filter","car","matchesType","matchesFuelType","totalPrice","start","Date","end","timeDiff","dayCount","methods","openAddCarModal","closeAddCarModal","addNewCar","alert","id","length","push","openModal","closeModal","confirmBooking","confirmation","window","confirm","booking","deleteCar","carId","saveEditedCar","index","findIndex","splice","closeEditModal","openEditModal"],"sources":["src/views/Cars.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1 class=\"text-center mb-4\">Cars</h1>\n\n    <!-- Filter by Car Type -->\n    <div class=\"form-group\">\n      <label for=\"carType\">Filter by Type:</label>\n      <select v-model=\"selectedType\" class=\"form-control\" id=\"carType\">\n        <option value=\"\">All</option>\n        <option value=\"COUPE\">COUPE</option>\n        <option value=\"SEDAN\">SEDAN</option>\n        <option value=\"HATCHBACK\">HATCHBACK</option>\n        <option value=\"SUV\">SUV</option>\n      </select>\n    </div>\n\n    <!-- Filter by Fuel Type -->\n    <div class=\"form-group mt-3\">\n      <label for=\"fuelType\">Filter by Fuel Type:</label>\n      <select v-model=\"selectedFuelType\" class=\"form-control\" id=\"fuelType\">\n        <option value=\"\">All</option>\n        <option value=\"Petrol\">Petrol</option>\n        <option value=\"Diesel\">Diesel</option>\n        <option value=\"Hybrid\">Hybrid</option>\n      </select>\n    </div>\n\n    <!-- Add New Car Button - Only for Admin -->\n    <div v-if=\"currentUser.role === 'admin'\" class=\"mb-4 text-center\">\n      <button class=\"btn btn-success\" @click=\"openAddCarModal\">\n        Add New Car\n      </button>\n    </div>\n\n    <!-- Displaying Cars -->\n    <div class=\"row mt-4\">\n      <div class=\"col-12 col-md-4\" v-for=\"car in filteredCars\" :key=\"car.id\">\n        <div class=\"card shadow-lg mb-4 rounded\">\n          <img\n              :src=\"require(`@/assets/images/${car.image}`)\"\n              class=\"card-img-top\"\n              alt=\"Car image\"\n          />\n          <div class=\"card-body\">\n            <h5 class=\"car-title\">\n              <span class=\"car-brand\">{{ car.brand }} </span>\n              <span class=\"car-model\">{{ car.model }}</span>\n            </h5>\n            <p class=\"card-text\">\n              <strong>Type:</strong> {{ car.type }} <br />\n              <strong>Year:</strong> {{ car.productionYear }} <br />\n              <strong>Price:</strong> ${{ car.price }} <br />\n              <strong>Location:</strong> {{ car.location }} <br />\n              <strong>Fuel Type:</strong> {{ car.fuelType }} <br />\n            </p>\n            <button class=\"btn btn-primary w-100\" @click=\"openModal(car)\">\n              Book Now\n            </button>\n            <button\n                v-if=\"currentUser.role === 'admin'\"\n                class=\"btn btn-warning w-100 mt-2\"\n                @click=\"openEditModal(car)\"\n            >\n              Edit\n            </button>\n            <button\n                v-if=\"currentUser.role === 'admin'\"\n                class=\"btn btn-danger w-100 mt-2\"\n                @click=\"deleteCar(car.id)\"\n            >\n              Delete\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Modal for Adding New Car -->\n    <div\n        v-if=\"showAddCarModal\"\n        class=\"modal fade show\"\n        tabindex=\"-1\"\n        role=\"dialog\"\n        aria-labelledby=\"addCarModalLabel\"\n        aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog\" role=\"document\">\n        <div class=\"modal-content card shadow-lg p-4 rounded-3 animate-fade-in\">\n          <div class=\"modal-header border-0\">\n            <h5 class=\"modal-title text-success\">Add New Car</h5>\n            <button\n                type=\"button\"\n                class=\"btn-close\"\n                @click=\"closeAddCarModal\"\n                aria-label=\"Close\"\n            ></button>\n          </div>\n          <div class=\"modal-body\">\n            <form @submit.prevent=\"addNewCar\">\n              <div class=\"row g-3\">\n                <div class=\"col-md-6\">\n                  <label for=\"newBrand\" class=\"form-label fw-bold\">Brand</label>\n                  <input\n                      type=\"text\"\n                      id=\"newBrand\"\n                      v-model=\"newCar.brand\"\n                      class=\"form-control border-success\"\n                      required\n                  />\n                </div>\n                <div class=\"col-md-6\">\n                  <label for=\"newModel\" class=\"form-label fw-bold\">Model</label>\n                  <input\n                      type=\"text\"\n                      id=\"newModel\"\n                      v-model=\"newCar.model\"\n                      class=\"form-control border-success\"\n                      required\n                  />\n                </div>\n                <div class=\"col-md-6\">\n                  <label for=\"newPrice\" class=\"form-label fw-bold\">Price</label>\n                  <input\n                      type=\"number\"\n                      id=\"newPrice\"\n                      v-model=\"newCar.price\"\n                      class=\"form-control border-success\"\n                      required\n                  />\n                </div>\n                <div class=\"col-md-6\">\n                  <label for=\"newType\" class=\"form-label fw-bold\">Type</label>\n                  <select\n                      v-model=\"newCar.type\"\n                      class=\"form-control border-success\"\n                      id=\"newType\"\n                      required\n                  >\n                    <option value=\"COUPE\">COUPE</option>\n                    <option value=\"SEDAN\">SEDAN</option>\n                    <option value=\"HATCHBACK\">HATCHBACK</option>\n                    <option value=\"SUV\">SUV</option>\n                  </select>\n                </div>\n                <div class=\"col-md-12\">\n                  <label for=\"newFuelType\" class=\"form-label fw-bold\"\n                  >Fuel Type</label\n                  >\n                  <select\n                      v-model=\"newCar.fuelType\"\n                      class=\"form-control border-success\"\n                      id=\"newFuelType\"\n                      required\n                  >\n                    <option value=\"Petrol\">Petrol</option>\n                    <option value=\"Diesel\">Diesel</option>\n                    <option value=\"Hybrid\">Hybrid</option>\n                  </select>\n                </div>\n              </div>\n            </form>\n          </div>\n          <div class=\"modal-footer justify-content-center border-0\">\n            <button\n                type=\"button\"\n                class=\"btn btn-outline-secondary rounded-pill\"\n                @click=\"closeAddCarModal\"\n            >\n              Close\n            </button>\n            <button\n                type=\"button\"\n                class=\"btn btn-success rounded-pill\"\n                @click=\"addNewCar\"\n            >\n              Add Car\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Modal for Editing Car Details -->\n    <div\n        v-if=\"showEditModal\"\n        class=\"modal fade show\"\n        tabindex=\"-1\"\n        role=\"dialog\"\n        aria-labelledby=\"editCarModalLabel\"\n        aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog\" role=\"document\">\n        <div class=\"modal-content card shadow-lg p-4 rounded-3 animate-fade-in\">\n          <div class=\"modal-header border-0\">\n            <h5 class=\"modal-title text-primary\">Edit Car Details</h5>\n            <button\n                type=\"button\"\n                class=\"btn-close\"\n                @click=\"closeEditModal\"\n                aria-label=\"Close\"\n            ></button>\n          </div>\n          <div class=\"modal-body\">\n            <form @submit.prevent=\"saveEditedCar\">\n              <div class=\"row g-3\">\n                <div class=\"col-md-6\">\n                  <label for=\"editBrand\" class=\"form-label fw-bold\"\n                  >Brand</label\n                  >\n                  <input\n                      type=\"text\"\n                      id=\"editBrand\"\n                      v-model=\"editedCar.brand\"\n                      class=\"form-control border-primary\"\n                      required\n                  />\n                </div>\n                <div class=\"col-md-6\">\n                  <label for=\"editModel\" class=\"form-label fw-bold\"\n                  >Model</label\n                  >\n                  <input\n                      type=\"text\"\n                      id=\"editModel\"\n                      v-model=\"editedCar.model\"\n                      class=\"form-control border-primary\"\n                      required\n                  />\n                </div>\n                <div class=\"col-md-6\">\n                  <label for=\"editPrice\" class=\"form-label fw-bold\"\n                  >Price</label\n                  >\n                  <input\n                      type=\"number\"\n                      id=\"editPrice\"\n                      v-model=\"editedCar.price\"\n                      class=\"form-control border-primary\"\n                      required\n                  />\n                </div>\n                <div class=\"col-md-6\">\n                  <label for=\"editType\" class=\"form-label fw-bold\">Type</label>\n                  <select\n                      v-model=\"editedCar.type\"\n                      class=\"form-control border-primary\"\n                      id=\"editType\"\n                      required\n                  >\n                    <option value=\"COUPE\">COUPE</option>\n                    <option value=\"SEDAN\">SEDAN</option>\n                    <option value=\"HATCHBACK\">HATCHBACK</option>\n                    <option value=\"SUV\">SUV</option>\n                  </select>\n                </div>\n                <div class=\"col-md-12\">\n                  <label for=\"editFuelType\" class=\"form-label fw-bold\"\n                  >Fuel Type</label\n                  >\n                  <select\n                      v-model=\"editedCar.fuelType\"\n                      class=\"form-control border-primary\"\n                      id=\"editFuelType\"\n                      required\n                  >\n                    <option value=\"Petrol\">Petrol</option>\n                    <option value=\"Diesel\">Diesel</option>\n                    <option value=\"Hybrid\">Hybrid</option>\n                  </select>\n                </div>\n              </div>\n            </form>\n          </div>\n          <div class=\"modal-footer justify-content-center border-0\">\n            <button\n                type=\"button\"\n                class=\"btn btn-outline-secondary rounded-pill\"\n                @click=\"closeEditModal\"\n            >\n              Close\n            </button>\n            <button\n                type=\"button\"\n                class=\"btn btn-primary rounded-pill\"\n                @click=\"saveEditedCar\"\n            >\n              Save Changes\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Modal for Booking Dates -->\n    <div\n        v-if=\"showModal\"\n        class=\"modal fade show\"\n        tabindex=\"-1\"\n        role=\"dialog\"\n        aria-labelledby=\"exampleModalLabel\"\n        aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog\" role=\"document\">\n        <div class=\"modal-content card shadow-lg rounded p-4 animate-fade-in\">\n          <div class=\"modal-body\">\n            <h6>Select your booking dates:</h6>\n            <div class=\"form-group\">\n              <label for=\"startDate\">From:</label>\n              <input\n                  type=\"date\"\n                  v-model=\"startDate\"\n                  class=\"form-control\"\n                  min=\"2025-01-01\"\n                  required\n              />\n            </div>\n            <div class=\"form-group\">\n              <label for=\"endDate\">To:</label>\n              <input\n                  type=\"date\"\n                  v-model=\"endDate\"\n                  class=\"form-control\"\n                  :min=\"startDate\"\n                  required\n              />\n            </div>\n            <!-- Display total price if it's available -->\n            <div v-if=\"totalPrice > 0\" class=\"mt-3\">\n              <strong>Total Price:</strong> ${{ totalPrice }}\n            </div>\n          </div>\n          <div class=\"modal-footer justify-content-center\">\n            <button\n                type=\"button\"\n                class=\"btn btn-secondary rounded-pill\"\n                @click=\"closeModal\"\n            >\n              Close\n            </button>\n            <button\n                type=\"button\"\n                class=\"btn btn-success rounded-pill\"\n                @click=\"confirmBooking\"\n                :disabled=\"\n                !startDate ||\n                !endDate ||\n                new Date(startDate) >= new Date(endDate)\n              \"\n            >\n              Confirm Booking\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {cars} from \"@/cars\";\n\nexport default {\n  name: \"CarList\",\n  data() {\n    return {\n      showAddCarModal: false,\n      newCar: {\n        brand: \"\",\n        model: \"\",\n        price: 0,\n        type: \"\",\n        fuelType: \"\",\n      },\n      cars:cars,\n\n      selectedType: \"\",\n      selectedFuelType: \"\",\n      showModal: false,\n      showEditModal: false,\n      selectedCar: null,\n      startDate: \"\",\n      endDate: \"\",\n      editedCar: {},\n      currentUser: {\n        role: \"admin\", // Читање од localStorage\n      },\n      myBookings: [],\n    };\n  },\n  computed: {\n    filteredCars() {\n      return this.cars.filter((car) => {\n        const matchesType =\n            !this.selectedType || car.type === this.selectedType;\n        const matchesFuelType =\n            !this.selectedFuelType || car.fuelType === this.selectedFuelType;\n        return matchesType && matchesFuelType;\n      });\n    },\n\n    totalPrice() {\n      if (this.startDate && this.endDate && this.selectedCar) {\n        const start = new Date(this.startDate);\n        const end = new Date(this.endDate);\n        const timeDiff = end - start;\n        const dayCount = timeDiff / (1000 * 3600 * 24);\n        if (dayCount < 1) return 0;\n        return this.selectedCar.price * dayCount;\n      }\n      return 0;\n    },\n\n  },\n  methods: {\n    openAddCarModal() {\n      this.showAddCarModal = true;\n      this.newCar = {\n        brand: \"\",\n        model: \"\",\n        price: \"\",\n        type: \"COUPE\",\n        fuelType: \"Petrol\",\n      };\n    },\n\n    closeAddCarModal() {\n      this.showAddCarModal = false;\n    },\n\n    addNewCar() {\n      if (\n          !this.newCar.brand ||\n          !this.newCar.model ||\n          !this.newCar.price ||\n          !this.newCar.type ||\n          !this.newCar.fuelType\n      ) {\n        alert(\"Please fill all fields.\");\n        return;\n      }\n\n      const newCar = {\n        id: this.cars.length + 1,\n        ...this.newCar,\n      };\n\n      // Додавање на новото возило\n      this.cars.push(newCar);\n\n      // Затворање на модалот\n      this.closeAddCarModal();\n\n      alert(`You have successfully added the ${newCar.brand} ${newCar.model}`);\n    },\n\n    openModal(car) {\n      this.selectedCar = car;\n      this.showModal = true;\n    },\n\n    closeModal() {\n      this.showModal = false;\n      this.startDate = \"\";\n      this.endDate = \"\";\n      this.selectedCar = null;\n    },\n\n    confirmBooking() {\n      if (!this.startDate || !this.endDate) {\n        alert(\"Please select both start and end dates for the booking.\");\n        return;\n      }\n\n      if (this.endDate < this.startDate) {\n        alert(\"End date cannot be earlier than start date.\");\n        return;\n      }\n\n      const confirmation = window.confirm(\n          `Are you sure you want to book from ${this.startDate} to ${this.endDate}?`\n      );\n\n      if (confirmation) {\n        // Dodaj ja rezervacijata vo \"myBookings\"\n        const booking = {\n          car: this.selectedCar,\n          startDate: this.startDate,\n          endDate: this.endDate,\n          totalPrice: this.totalPrice,\n        };\n        this.myBookings.push(booking); // Dodavanje vo lista\n        alert(\n            `You have successfully booked the ${this.selectedCar.brand} ${this.selectedCar.model}`\n        );\n        this.closeModal();\n      } else {\n        alert(\"Booking canceled.\");\n      }\n    },\n\n    deleteCar(carId) {\n      this.cars = this.cars.filter((car) => car.id !== carId);\n    },\n\n    saveEditedCar() {\n      const index = this.cars.findIndex((car) => car.id === this.editedCar.id);\n      if (index !== -1) {\n        this.cars.splice(index, 1, { ...this.editedCar });\n      }\n      this.showEditModal = false;\n    },\n\n    closeEditModal() {\n      this.showEditModal = false;\n    },\n\n    openEditModal(car) {\n      this.editedCar = { ...car };\n      this.showEditModal = true;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 1200px;\n  margin: 0 auto;\n  margin-top: 6rem;\n}\n\n.card-body {\n  padding: 20px;\n}\n\n.car-title {\n  font-size: 1.25rem;\n  font-weight: bold;\n}\n\n.car-brand {\n  color: #007bff;\n}\n\n.car-model {\n  color: #6c757d;\n}\n\n.card {\n  transition: transform 0.2s ease-in-out;\n}\n\n.card:hover {\n  transform: scale(1.05);\n}\n\n.modal-backdrop {\n  display: none;\n}\n\n.modal {\n  display: block;\n}\n\n.modal .modal-dialog {\n  margin-top: 10%;\n  margin-bottom: 10%;\n  max-width: 500px;\n}\n\n.mt-3 .mb-4{\n  margin-top: 1rem !important;\n  margin-bottom: 1rem !important;\n}\n\n</style>\n"],"mappings":";;;AAuWA,SAAAA,IAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,eAAA;MACAC,MAAA;QACAC,KAAA;QACAC,KAAA;QACAC,KAAA;QACAC,IAAA;QACAC,QAAA;MACA;MACAT,IAAA,EAAAA,IAAA;MAEAU,YAAA;MACAC,gBAAA;MACAC,SAAA;MACAC,aAAA;MACAC,WAAA;MACAC,SAAA;MACAC,OAAA;MACAC,SAAA;MACAC,WAAA;QACAC,IAAA;MACA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACAC,aAAA;MACA,YAAAtB,IAAA,CAAAuB,MAAA,CAAAC,GAAA;QACA,MAAAC,WAAA,GACA,MAAAf,YAAA,IAAAc,GAAA,CAAAhB,IAAA,UAAAE,YAAA;QACA,MAAAgB,eAAA,GACA,MAAAf,gBAAA,IAAAa,GAAA,CAAAf,QAAA,UAAAE,gBAAA;QACA,OAAAc,WAAA,IAAAC,eAAA;MACA;IACA;IAEAC,WAAA;MACA,SAAAZ,SAAA,SAAAC,OAAA,SAAAF,WAAA;QACA,MAAAc,KAAA,OAAAC,IAAA,MAAAd,SAAA;QACA,MAAAe,GAAA,OAAAD,IAAA,MAAAb,OAAA;QACA,MAAAe,QAAA,GAAAD,GAAA,GAAAF,KAAA;QACA,MAAAI,QAAA,GAAAD,QAAA;QACA,IAAAC,QAAA;QACA,YAAAlB,WAAA,CAAAP,KAAA,GAAAyB,QAAA;MACA;MACA;IACA;EAEA;EACAC,OAAA;IACAC,gBAAA;MACA,KAAA/B,eAAA;MACA,KAAAC,MAAA;QACAC,KAAA;QACAC,KAAA;QACAC,KAAA;QACAC,IAAA;QACAC,QAAA;MACA;IACA;IAEA0B,iBAAA;MACA,KAAAhC,eAAA;IACA;IAEAiC,UAAA;MACA,IACA,MAAAhC,MAAA,CAAAC,KAAA,IACA,MAAAD,MAAA,CAAAE,KAAA,IACA,MAAAF,MAAA,CAAAG,KAAA,IACA,MAAAH,MAAA,CAAAI,IAAA,IACA,MAAAJ,MAAA,CAAAK,QAAA,EACA;QACA4B,KAAA;QACA;MACA;MAEA,MAAAjC,MAAA;QACAkC,EAAA,OAAAtC,IAAA,CAAAuC,MAAA;QACA,QAAAnC;MACA;;MAEA;MACA,KAAAJ,IAAA,CAAAwC,IAAA,CAAAp